.logs {
  border: 1rpx solid #e0e0e0;
  border-radius: 10rpx;
  box-shadow: 0 0 5rpx #e0e0e0;
}
.item {
  padding: 30rpx;
  border-bottom: 1rpx solid #00b26a;
  font-size: 30rpx;
  color:#888;
}
.item:last-child {
  border-bottom: 0;
}
.timestamp {
  display: block;
  margin-top: 20rpx;
}
.name {
  display: inline-block;
  width: 70%;
  color: #00b26a;
}
.action {
  display: inline-block;
  width: 30%;
  color: #f40;
  text-align: right;
}
